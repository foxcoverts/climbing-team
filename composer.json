{
    "name": "laravel/laravel",
    "type": "project",
    "description": "Management website for an activity team",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.ralphjsmit.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "eluceo/ical": "^2.13",
        "filament/filament": "^3.2",
        "laravel/framework": "^11.6",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "league/flysystem-read-only": "^3.23",
        "log1x/blade-filetype-icons": "^1.0",
        "propaganistas/laravel-phone": "^5.3",
        "ralphjsmit/laravel-filament-activitylog": "^1.8",
        "sabre/vobject": "^4.5",
        "spatie/laravel-activitylog": "^4.9",
        "symfony/http-client": "^7.0",
        "symfony/sendgrid-mailer": "^7.0",
        "symfony/yaml": "^7.0",
        "ysfkaya/filament-phone-input": "^3.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13",
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.0",
        "laravel/pint": "^1.15",
        "laravel/sail": "^1.29",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.1",
        "spatie/laravel-ignition": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "ralphjsmit/packages": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
